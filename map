           };
		
      map = new google.maps.Map(document.getElementById( "map-canvas" ), mapOptions );

      var  marker = [ ];

      marker.push(
        new google.maps.Marker( {
          position: new google.maps.LatLng( 35.78, -78.80 ),
          map: map,
          title: "Cary, NC"
        } )
		      );
		marker.push(
        new google.maps.Marker( {
          position: new google.maps.LatLng( 35.95, -77.81 ),
          map: map,
          icon: {
