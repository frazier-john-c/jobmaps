            fillColor: "red",
            fillOpacity: 0.7,
            scale: 10,
            strokeColor: "black",
            strokeWeight: 1
          },
          title: "SVG circle",
        } ))
		var myLatLng = new google.maps.LatLng(35.890542, -78.274856);
		var beachMarker = new google.maps.Marker({
			position: myLatLng,
			map: map,
			icon: 'images/office.png'
				});
    }

    google.maps.event.addDomListener( window, "load", init );
